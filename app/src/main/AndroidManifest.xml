<?xml version="1.0" encoding="utf-8"?>
<manifest package="io.github.easyintent.quickref"
          xmlns:android="http://schemas.android.com/apk/res/android"
          android:installLocation="auto">

    <application
        android:allowBackup="true"
        android:name=".QuickRefApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="io.github.easyintent.quickref.MainActivity"
            android:label="@string/app_name"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name="io.github.easyintent.quickref.QuickRefActivity"/>
    </application>

</manifest>